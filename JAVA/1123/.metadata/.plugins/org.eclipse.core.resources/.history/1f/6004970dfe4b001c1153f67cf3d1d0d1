import java.util.Scanner;

public class test01 {
	public static void main(String[] args) {
	
		Scanner sc = new Scanner(System.in);
		int money;
		int time;
		
		System.out.print("시급 : ");
		money = sc.nextInt();
		System.out.print("근무 시간 : ");
		time = sc.nextInt();
		
		System.out.println("총 급여 : "+money*time);
		
		
	}
}
