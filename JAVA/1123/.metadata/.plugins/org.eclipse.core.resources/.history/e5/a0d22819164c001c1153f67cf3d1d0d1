
public class Operator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int a, b;
		a= 7;
		b =2;
		System.out.printf("%d + %d = %d\n",a , b, a+b);
		System.out.printf("%d - %d = %d\n",a , b, a-b);
		System.out.printf("%d * %d = %d\n",a , b, a*b);
		System.out.printf("%d / %d = %d\n",a , b, a/b);
		System.out.printf("%d %% %d = %d\n",a , b, a%b);
	}

}
