
public class BreakTest02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		final int MAX = 100;
		
		int degree =1;;
		for(int i =0; i< MAX; i++) {
			for(int j =0; j<10; j++) {
				System.out.printf("%2f  ", (degree*1.8)+32);
				degree++;
			}
			System.out.println();
		}
		
	}

}
